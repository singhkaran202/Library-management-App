<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<AnchorPane prefHeight="400.0" prefWidth="600.0"
	xmlns="http://javafx.com/javafx/17"
	xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.employeemanagementsystem1.BookIssueController">
	<children>
		<Text layoutX="188.0" layoutY="46.0" strokeType="OUTSIDE" strokeWidth="0.0" text="BOOK ISSUE FORM">
			<font>
				<Font name="Bodoni MT" size="36.0" />
			</font>
		</Text>
		<TextField fx:id="bookIdField" layoutX="171.0" layoutY="121.0" prefHeight="25.0" prefWidth="258.0" promptText="Book ID" />
		<TextField fx:id="daysField" layoutX="171.0" layoutY="156.0" prefHeight="25.0" prefWidth="258.0" promptText="No of Days" />
		<TextField fx:id="customerNameField" layoutX="171.0" layoutY="191.0" prefHeight="25.0" prefWidth="258.0" promptText="Customer Name" />
		<TextField fx:id="mobileNumberField" layoutX="171.0" layoutY="226.0" prefHeight="25.0" prefWidth="258.0" promptText="Mobile Number" />
		<TextField fx:id="emailField" layoutX="171.0" layoutY="261.0" prefHeight="25.0" prefWidth="258.0" promptText="Email" />
		<TextField fx:id="authorField" layoutX="171.0" layoutY="296.0" prefHeight="25.0" prefWidth="258.0" promptText="Author" />
		<Button fx:id="issueButton" layoutX="234.0" layoutY="346.0" mnemonicParsing="false" onAction="#handleIssueButtonAction" text="Issue" />
		<Button layoutX="330.0" layoutY="346.0" mnemonicParsing="false" onAction="#handleCancelButtonAction" text="Cancel" />
	</children>
</AnchorPane>
